<div class = "outer-card">
	<div class = "card" id="card_<%= dish.name %>">
		<div class="front side">
			<div class = "card_pic">
				Photo goes here
			</div>
			<div id="card_desc">
				<%= link_to dish.name, "/#{@restaurant.url}/#{dish.url}" %>
			</div>
		</div>

		<div class="back side">
			<div class="dish_back">
				<%= dish.restaurant.name %><br>
				<%= dish.name %><br><br>
				<%= dish.description %><br>
				<%= link_to "View More Images", "/#{@restaurant.url}/#{dish.url}" %>
			</div>
		</div>
	</div>
</div>