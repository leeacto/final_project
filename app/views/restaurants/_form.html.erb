<%= form_for restaurant do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %><br>

	<%= f.label :address %>
	<%= f.text_field :address %><br>

	<%= f.label :city %>
	<%= f.text_field :city %><br>

	<%= f.label :state %>
	<%= f.text_field :state %><br>

	<%= f.label :zip %>
	<%= f.text_field :zip %><br>

	<%= f.label :cuisine %>
	<%= f.text_field :cuisine %><br>

	<%= f.submit %>
<% end %>