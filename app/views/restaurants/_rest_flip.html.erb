<div class = "card" id="card-<%= index %>">
	<div class="front">
		<div class = "card_pic">
			Photo goes here
		</div>
		<div id="card_desc">
				<%=link_to restaurant.name, "/#{restaurant.url}/dishes" %> - 
				<%= restaurant.cuisine %>

		</div>
	</div>

	<div class="back">
			<span class="friend-name">
				<div class = "rest_text">
					<div class="rest_top"
						<%= restaurant.name %><br>
						<%= restaurant.cuisine %><br><br>
					</div>
					<div class = "rest_bot">
						<%= restaurant.address %><br>
						<%= restaurant.city %>, <%= restaurant.state %><br>
						<%= restaurant.zip %><br>
						<%= link_to "View Menu", "/#{restaurant.url}/dishes" %>
					</div>
				</div>
				<div class = "gmap" id="map-canvas-<%= index %>"></div>
			</span>
	  <div class="actions">
		</div>
	</div>
</div>